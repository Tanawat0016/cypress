{
  "reporter": "mochawesome",
  "baseUrl": "http://localhost:8080",
  "ignoreTestFiles": ["*.md"],
  "watchForFileChanges": true,
  "reporterOptions": {
    "reportFilename":"test_result",
    "reportDir": "results/",
    "overwrite": false,
    "html": true,
    "json": true
  },
  "video": false,
  "chromeWebSecurity": false,

  "env":{
    "RETRIES" : 3
  }
}
